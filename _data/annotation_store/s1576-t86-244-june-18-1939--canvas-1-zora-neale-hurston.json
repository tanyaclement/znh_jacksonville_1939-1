        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "s1576-t86-244-june-18-1939--canvas-1-zora-neale-hurston-annotation-17.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Oh the Buford Boat Done Come' is a song from the Geechee country in South Carolina but I heard it down in Florida from a Geechee that moved down in Florida. I forget her name right now.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://benwbrum.github.io/znh_jacksonville_1939/s1576-t86-244-june-18-1939-/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "972.985537,983.057851"
        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "s1576-t86-244-june-18-1939--canvas-1-zora-neale-hurston-annotation-18.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "It's a little dance song with a Charleston rhythm.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://benwbrum.github.io/znh_jacksonville_1939/s1576-t86-244-june-18-1939-/canvas-1/canvas",
        "selector": {
          "type": "PointSelector",
          "t": "984.142562"
        }
      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
