      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston laughs [distortion]",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
