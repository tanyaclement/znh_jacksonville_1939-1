    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "s1576-t86-244-june-18-1939--canvas-1-zora-neale-hurston-annotation-16.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "I know I know some more verses but right off I don't recall.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://benwbrum.github.io/znh_jacksonville_1939/s1576-t86-244-june-18-1939-/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
