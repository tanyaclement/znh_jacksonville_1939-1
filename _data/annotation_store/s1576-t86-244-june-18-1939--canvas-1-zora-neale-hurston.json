    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "s1576-t86-244-june-18-1939--canvas-1-zora-neale-hurston-annotation-21.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Oh, just any group, any working group, and they'll clap their hands on it and sing.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
