        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Well, I heard the first verses, I got it in my native village of Eatonville, Florida from George Thomas.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://benwbrum.github.io/znh_jacksonville_1939/s1576-t86-244-june-18-1939-/canvas-1/canvas",
        "selector": {
          "type": "RangeSelector",
          "t": "38.119835,43.272211"
        }
      }
    },
    {
