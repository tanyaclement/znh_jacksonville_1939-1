      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "It's one of these things that's grown by incremental repetition until perhaps it's the longest song in America.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
