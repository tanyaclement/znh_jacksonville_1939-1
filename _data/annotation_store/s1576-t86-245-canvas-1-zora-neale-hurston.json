    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "s1576-t86-245-canvas-1-zora-neale-hurston-annotation-4.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "The fire dance is some sort of African survival in the West Indies and they beat the drums and sing these little songs.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://tanyaclement.github.io/znh_jacksonville_1939/s1576-t86-245/canvas-1/canvas",
        "selector": {
          "type": "PointSelector",
