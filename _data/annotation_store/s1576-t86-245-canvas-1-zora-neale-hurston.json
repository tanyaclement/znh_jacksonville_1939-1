      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Well, I was doing research down there, collecting songs out of Columbia University and I collected quite a few of them and this is just one of them.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Zora Neale Hurston",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
        "source": "https://tanyaclement.github.io/znh_jacksonville_1939/s1576-t86-245/canvas-1/canvas",
        "selector": {
          "type": "PointSelector",
          "t": "736.535834"
        }
