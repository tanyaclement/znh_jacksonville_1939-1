      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": " Uh, it was called . . . it didn't have no particular name. They called it 'the Captain's Mule.'",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Harold B. Hazelhurst",
          "format": "text/plain",
