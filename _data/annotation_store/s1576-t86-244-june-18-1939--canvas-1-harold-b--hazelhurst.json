      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Well, they'd be double-driving.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Harold B. Hazelhurst",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
