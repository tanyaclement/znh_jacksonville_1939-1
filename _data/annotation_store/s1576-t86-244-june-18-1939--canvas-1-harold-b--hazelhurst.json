      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "Sing the song as the mules were going up and down the grade, loading.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Harold B. Hazelhurst",
          "format": "text/plain",
          "purpose": "tagging"
