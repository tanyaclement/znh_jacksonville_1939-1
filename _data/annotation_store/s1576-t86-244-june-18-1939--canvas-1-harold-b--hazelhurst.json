      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": [
        {
          "type": "TextualBody",
          "value": "I was water boy on the hard road camp and the men during the day would sing these songs while they were working.",
          "format": "text/plain",
          "purpose": "commenting"
        },
        {
          "type": "TextualBody",
          "value": "Harold B. Hazelhurst",
          "format": "text/plain",
          "purpose": "tagging"
        }
      ],
      "target": {
